{
  "name": "johnnymast/framework",
  "description": "My personal framework",
  "minimum-stability": "stable",
  "type": "library",
  "license": "MIT",
  "keywords": [
  ],
  "autoload": {
    "psr-4": {
      "App\\Framework\\": "src/framework/",
      "App\\Framework\\Tests\\": "tests/"
    },
    "files": [
      "src/Framework/Helpers/Helpers.php"
    ]
  },
  "require": {
    "slim/slim": "4.*",
    "slim/psr7": "^1.6",
    "php-di/slim-bridge": "^3.3",
    "vlucas/phpdotenv": "^5.5",
    "eftec/bladeone": "^4.9",
    "zeuxisoo/slim-whoops": "^0.7.3",
    "symfony/console": "^6.2",
    "phpmailer/phpmailer": "^6.8",
    "twig/twig": "^3.0",
    "monolog/monolog": "^3.5"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10",
    "pestphp/pest": "^2.6",
    "symfony/var-dumper": "^6.3"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
